<!-- browse.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Community</title>
</head>
<body>
    
    
    <h1>Browse Community</h1>
    <ul>
        <% articles.forEach(article => { %>
        <li>
            <a href="/article?id=<%= article.artid %>"><%= article.title %></a>
        </li>
        <% }); %>
    </ul>
    <a href="/createarticle">create</a>
</body>
</html>
